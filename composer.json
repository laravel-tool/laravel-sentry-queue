{
    "name": "laravel-tool/laravel-sentry-queue",
    "description": "Laravel Sentry Queue",
    "type": "laravel-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Potemkin",
            "email": "ysoft2000@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "sentry/sentry-laravel": "^3.1",
        "laravel/framework": "^9.0",
        "illuminate/queue": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTool\\LaravelSentryQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelTool\\LaravelSentryQueue\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTool\\LaravelSentryQueue\\ServiceProvider"
            ]
        }
    }
}
